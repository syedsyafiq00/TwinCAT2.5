	IF ARrStnTempSV[6] >= ARrStnChillerValueTempSetpointFinal[6] THEN
	ARbStnChillerValueOnOff[6] := TRUE;
	ELSE 
	ARbStnChillerValueOnOff[6] := FALSE;
	END_IF

END_IF


ARwDO[eY13010_STN06CHILLEDWATERSUPPLYAV6_2].0 := ARbStnChillerValueOnOff[6];




]]></ST>
