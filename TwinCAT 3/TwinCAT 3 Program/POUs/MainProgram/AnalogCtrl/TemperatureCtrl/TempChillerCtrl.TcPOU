    <Declaration><![CDATA[PROGRAM TempChillerCtrl
VAR
	ARrStnChillerValueTempSetpointFinal: ARRAY [1..iStnQty] OF REAL;
	
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[
(*Stn 3 Chiller Value Ctrl*)
IF bAutoPreparationPb  THEN
	ARrStnChillerValueTempSetpointFinal[3] := ARrStnTempSV[3] + ARrStnChillerValueTempSetpoint[3];
	
	IF ARrStnTempSV[3] >= ARrStnChillerValueTempSetpointFinal[3] THEN
	ARbStnChillerValueOnOff[3] := TRUE;
	ELSE 
	ARbStnChillerValueOnOff[3] := FALSE;
	END_IF

END_IF


ARwDO[eY13005_STN03CHILLEDWATERSUPPLYAV3_3].0 := ARbStnChillerValueOnOff[3];


(*Stn 6 Chiller Value Ctrl*)
